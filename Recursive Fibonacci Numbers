//Recursive fibonacci numbers

#include <iostream>

using namespace std;

int fib(int);

int main()
{
  cout << fib(10) << endl; //Result give 55 according to fibonacci numbers.
}

//Recursive fibonacci numbers
int fib(int x)
{
	if(x == 0)
	{
		return 0;
	}
	if(x == 1)
	{
		return 1;
	}

	return fib(x-1) + fib(x-2);

}
